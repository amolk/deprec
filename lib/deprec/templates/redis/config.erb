daemonize yes
pidfile /var/run/redis_<%= redis_port %>.pid
logfile /var/log/redis.log

port <%= redis_port %>
bind <%= redis_host %>
timeout 300

loglevel notice

## Default configuration options
databases 16

save 900 1
save 300 10
save 60 10000

rdbcompression yes
dbfilename redis.rdb

dir /opt/redis/
appendonly no

glueoutputbuf yes

